/*
	proscom.ru
	Простые Коммуникации
*/
@import "common.styl"



//	Основные стили
// ===============================================
min-height = "screen and (min-height: 768px)"
body
	position relative; 
	min-height 600px; 
	background #f0f0f0;
#shikarniy
	position relative; min-width 1280px; min-height 100%; clearfix();
	.wrap
		position relative; min-width 1260px; max-width 1920px; margin 0 auto; height 100%;
	.main-page
		padding 95px 30px 30px 293px;
	.main-width
		max-width 1100px; margin 0 auto;
.hoverbg
	.icon
		position relative; 
		.act
			display none; position absolute; left 0; top 0;

.text-pop
	.ins
		width 500px; 
		p
			margin-bottom 25px;
			text-align center;
	.close
		text-align center;
		a
			padding 15px 45px;


//	Левая колонка
// ===============================================
.sidebar
	position absolute; left 0px; top 0px; 
	background #272a33; 
	width 263px; min-height 750px; height 100%; 
	z-index 101; text-align center;
	@media min-height
		position fixed; left 0; top 0;
	.logo
		position fixed; left 0; top 0; width 263px; z-index 5; 
		height 21px; padding 22px 0; background #272a33;
		a
			display inline-block; width 178px; height 16px; anim();
			background url("../img/left/logo.png") no-repeat; anim-op();
			&:hover
				opacity 0.8; tdn();
	//=====
	.user
		position relative; padding 93px 0 18px 0; background #1d2028; text-align center;
		.pic
			display inline-block; background #272a33; padding 7px; margin-bottom 7px; border-radius 50%; 
			img
				display block; 
		.age
			position absolute; right 30px; top 93px; height 17px; padding-left 23px; color #767886; font-size 12px;
			&.man
				background url("../img/left/i-man.png") no-repeat; 
			&.woman
				height 19px
				padding-left 20px
				background url("../img/left/i-woman.png") no-repeat; 
		a.settings
			position absolute; left 57px; top 93px;
			.icon
				display block; width 17px; height 19px; background url("../img/left/i-setting.png") no-repeat; anim();
				&:hover
					transform rotate(90deg); tdn();
				.act
					width 17px; height 19px; background url("../img/left/i-setting-a.png") no-repeat;
					
		.name
			padding-bottom 20px; color #fff;  text-transform uppercase;
		.contacts
			display inline-block; color #989aa8; 
			a
				color #989aa8;
				&:hover
					color #3cb4c7; tdn();
			div
				 padding-left 27px; margin-bottom 8px; text-align left; 
			.phone
				background url("../img/left/i-phone-g.png") 2px center no-repeat;
			.email
				background url("../img/left/i-email-g.png") 0 center no-repeat; 
			.site
				background url("../img/left/i-site-g.png") 0 center no-repeat; 
	//=====
	.menu
		background #272a33; text-align left; 
		ul
			lino();
			li
				anim-bgcolor();
				a
					display block; padding 19px 30px 19px 60px; color #989aa8; text-transform uppercase;
				&:hover
					background #22252d url("../img/left/menu-arrow.png") right center no-repeat; 
					a
						tdn();
			li.home a
				background url("../img/left/menu-home.png") 32px 19px no-repeat; 
			li.career a
				background url("../img/left/menu-career.png") 32px 19px no-repeat;
			li.work a
				background url("../img/left/menu-work.png") 32px 20px no-repeat;
			li.educ a
				background url("../img/left/menu-educ.png") 32px 19px no-repeat;
			li.educ-ext a
				background url("../img/left/menu-educ-ext.png") 32px 19px no-repeat;
			li.active
				background #1d2028 url("../img/left/menu-arrow.png") right center no-repeat; border-left 3px solid #3bb3c7;  
				a
					margin-left -3px;
			li.vac a
				background-position 32px center; 
				
	//=====
	.logo-ibs
		position absolute; left 100px; bottom 60px;
		a
			display block; width 49px; height 18px; background url("../img/main/logo-ibs.png") no-repeat; anim-op();
			&:hover
				tdn();
				opacity 0.7;
	//=====
	.enter
		position relative;
		.login
			padding 93px 30px 25px 30px;
			text-align center;
			background #1d2028;
			.bname
				color #fff; text-transform uppercase; semibold();
				padding-bottom 20px;  
			input
				width 100%; full(); margin-bottom 15px; padding 10px 15px 10px 45px;
				font-size 14px; color #fff; text-align left;
				border none; border-radius 25px; 
				&.login
					background #272a33 url("../img/main/i-user.png") 20px center no-repeat;
				&.pas
					background #272a33 url("../img/main/i-key.png") 20px center no-repeat;
				&:hover, &:focus
					background-color #333743; 
			.actions
				text-align left;
				clearfix(); 
				a
					float left; 
					display block;
					margin-top 7px;  
					color #989aa8; font-size 12px;
				input
					float right; 
					display inline-block; 
					width auto; margin-left 15px; padding 5px 20px; margin 0;
					font-size 14px; semibold();
					background #35a3b5; border-radius 25px;
					anim();
					&:hover
						background #3db4c7;
		//=====
		.reg
			padding 25px 30px 0 30px;
			text-align center;
			a
				display inline-block; 
				width 100%; full(); margin-left 15px; padding 5px 20px; margin 0;
				font-size 14px; semibold(); color #fff; tdn();
				background #35a3b5; border-radius 25px;
				anim();
				&:hover
					background #3db4c7; 


//	Поиск, полоска
// ===============================================
.topper
	position fixed; left 0; top 0; width 100%; min-width 650px; full(); height 65px; padding-left 263px; 
	z-index 100;
	background #3eb4c6; clearfix();
	background linear-gradient(to bottom, #3cb4c7 0%,#39abbd 100%);
	.main-width
		max-width 1160px;
	.search
		overflow hidden; full(); padding-top 13px; padding-left 30px; padding-right 5%;
		form
			position relative; 
			input.filed
				width 100%; full(); padding 8px 20px; color #fff; 
				background #35a3b5; border none; border-radius 50px; anim-long();
				&:hover, &:focus
					color #82868e; background #fff;
			input.filed::-webkit-input-placeholder
				color #fff; anim-long();
			input.filed:hover::-webkit-input-placeholder
				color #82868e;
			input.filed:focus::-webkit-input-placeholder
				color #82868e;
			input.submit
				position absolute; right 15px; top 10px; width 17px; height 18px; padding 0; 
				background url("../img/main/i-search.png") no-repeat; border none; anim-long();
			input.filed:hover + input.submit, input.filed:focus + input.submit
				background url("../img/main/i-search-a.png") no-repeat;
	.info
		float right; width 200px;
		a
			anim();
		a:hover
			background #38a9bb; tdn();
		.notafication
			float right; full(); padding 0 0px 0 0; white-space nowrap; 
			a
				display block; padding 25px 25px 18px 25px; clearfix();
				.icon
					display block; float left; width 16px; height 18px; background url("../img/main/i-count.png") no-repeat; 
				.count
					display block; float left; margin -5px 0 0 5px; padding 5px 5px;
					color #fff; font-size 12px; text-align center; 
					background #3a98a7; border-radius 3px;
			a:hover
				.icon
					animation notafication 0.7s;
		.exit
			float right; full(); padding 0px 0px 0 0;
			a
				display block; padding 25px 30px 18px 30px;
				.icon
					width 16px; height 18px; background url("../img/main/i-exit.png") no-repeat;
	.menu-wrap
		margin 0 auto
		padding 5px 0
		max-width 1160px
		td
			padding 0 30px


//	Рейтинг
// ===============================================
.jq-rating
	overflow hidden;
	.jq-rating-group
		cursor pointer;
		.jq-rating-star
			width 8px; height 8px; margin-right 13px; background #c8cbd0; border-radius 8px;
	.jq-rating-group--hover
		.jq-rating-star
			width 8px; height 8px; margin-right 13px; background #3cb4c7; border-radius 8px;
//=====
.rating
	ul
		lino(); clearfix();
		li
			float left; width 8px; height 8px; margin-right 13px; background #c8cbd0; border-radius 8px;
		li.full
			color #000; background #3cb4c7;



// ==========
.specbox
	position relative; padding 30px 55px 30px 50px; margin-bottom 15px;
	color #fff; font-size 16px; line-height 24px;
	background #3cb4c7; border-radius 3px; 
	&:after
		position absolute; right 30px; bottom 30px; width 11px; height 7px; content " ";
		background url("../img/main/i-q-right.png") no-repeat;
	&:before
		position absolute; left 30px; top 30px; width 11px; height 7px; content " ";
		background url("../img/main/i-q-left.png") no-repeat;


// ==========
.user-welcome
	position relative; margin -30px -30px 30px -30px;	
	.wrapper
		position relative; height 100%; max-width 1100px; margin 0 auto; clearfix();
	.inform
		position relative; height 354px; color #fff; padding 0 30px; 
		background url("../img/content/user-welcome.png") center center no-repeat; background-size cover;
		&.bg-profile-2
			background url("../img/content/user-welcome-2.jpg") center center no-repeat; background-size cover;
		.city
			position absolute; left 0; top 30px; padding-left 20px; 
			background url("../img/main/i-location.png") 0 center no-repeat;
		.educ
			position absolute; left 0; top 30px;
			.item
				padding-left 20px; padding-bottom 15px;
				background url("../img/main/i-educ-b.png") 0 2px no-repeat;
		.menu
			position absolute; right 0px; top 30px;
			.icon
				display block; width 17px; height 12px; background url("../img/main/i-menu-w.png") no-repeat;
				.act
					width 17px; height 12px; background url("../img/main/i-menu-a.png") no-repeat;
		.phone
			position absolute; left 0; bottom 30px; padding-left 20px; 
			background url("../img/main/i-phone-b.png") 0 center no-repeat; 
		.email
			position absolute; right 0; bottom 30px; padding-left 20px; 
			background url("../img/main/i-phone-b.png") 0 center no-repeat;
			a
				color #fff;
				&:hover
					color #3bb3c7; tdn();
		.user
			position absolute; left 50%; top 40px; width 500px; margin-left: -250px; text-align center;
			.pic
				position relative; display inline-block; padding-top 5px;
				img
					display inline-block; border 7px solid rgba(39,42,51, 0.2); border-radius 135px;
			.write
				position absolute; left -5px; top 0;
				.icon
					display block; width 25px; height 14px; background url("../img/main/i-msg-b.png") no-repeat;
					.act
						width 25px; height 14px; background url("../img/main/i-msg-b-a.png") no-repeat;
			.favorite
				position absolute; right -5px; top 0;
				.icon
					display block; width 18px; height 17px; background url("../img/main/i-star-b.png") no-repeat;
					.act
						width 18px; height 17px; background url("../img/main/i-star-b-a.png") no-repeat;
			.name
				padding 19px 0;
				a
					color #fff; text-transform uppercase; semibold();
					&:hover
						color #3bb3c7; tdn();
			.give-job
				a
					display inline-block; color #fff; padding 5px 13px 5px 40px; white-space nowrap; anim();
					background rgba(0,0,0,0.15) url("../img/main/i-work-b.png") 15px center no-repeat;
					&:hover
						color #3bb3c7; tdn();
						background rgba(0,0,0,0.20) url("../img/main/i-work-b.png") 15px center no-repeat;
			.site
				a
					display inline-block; color #fff; padding 5px 13px 5px 40px; white-space nowrap; anim();
					background rgba(0,0,0,0.15) url("../img/main/i-site-b.png") 15px center no-repeat;
					&:hover
						color #3bb3c7; tdn();
						background rgba(0,0,0,0.20) url("../img/main/i-site-b.png") 15px center no-repeat;				
	//=====
	.profile
		padding 0 30px 4px 30px; background #fff url("../img/main/user-welcome-profile.png") 0 bottom repeat-x;   
		.box
			float left; width 33%; padding 20px 0 20px 30px; full(); line-height 20px;
			border-right 1px solid #e5e5e5; 
			&:first-child 
				padding-left 0;
			&:last-child 
				border none;
			.name
				semibold();
			.content				
				span
					color #a5a7ac;


//	ГРАФИК
// ===============================================
.graphbox
	position relative; margin-bottom 15px; overflow hidden;
	background #fff; border-radius 5px; border 1px solid #e1e1e1; box-shadow: 0px 1px 1px 0.00px rgba(225, 225, 225, 0.75); 
	.topbl
		position relative; padding 40px 75px 30px 75px; clearfix();
		background #fdfdfd; border-bottom 1px solid #e2e2e2; 
		.slider-nav
			position absolute; top 50px;
			&.left
				left 30px;
				.icon
					background url("../img/main/i-arrow-left.png") no-repeat;
					.act
						background url("../img/main/i-arrow-left-a.png") no-repeat;
			&.right
				right 30px;
				.icon
					background url("../img/main/i-arrow-right.png") no-repeat;
					.act
						background url("../img/main/i-arrow-right-a.png") no-repeat;
			.icon
				display block; width 11px; height 19px;
				.act
					width 11px; height 19px;	
		.bg
			position absolute; left 50%; bottom -22px; width 953px; height 22px; margin-left -476px; z-index 1;
			background url("../img/main/graph-bg-top.png") no-repeat;
		.target
			float left; padding-left 40px; background url("../img/main/i-target.png") 0 center no-repeat;
			.name
				text-transform uppercase; semibold();
		.profit
			float right; text-align right;
			.name
				padding-bottom 5px; text-transform uppercase; semibold();
			.desc
				display inline-block; white-space nowrap; clearfix(); text-align right; 
				.va
					float left; padding-right 5px; padding-top 3px;
				.sep
					float left; width 3px; height 3px; margin 13px 0 0 10px; 
					background #b0b3ba; border-radius 3px; 
				.count
					float left; margin-left 10px; padding 5px 8px; color #fff; font-size 12px;
					background #b0b3ba; border-radius 3px;
	//=====
	.botbl
		position relative; padding 40px 75px 40px 75px; clearfix();
		background #fdfdfd; border-top 1px solid #e2e2e2; 
		.bg
			position absolute; left 50%; top -22px; width 953px; height 22px; margin-left -476px; z-index 1;
			background url("../img/main/graph-bg-bot.png") no-repeat;
		.target
			float left; padding-left 40px; background url("../img/main/i-target.png") 0 center no-repeat;
			.name
				text-transform uppercase; semibold();
		.profit
			float right; text-align right;
			.name
				padding-bottom 5px; text-transform uppercase; semibold();
			.desc
				display inline-block; white-space nowrap; clearfix(); text-align right; 
				.va
					float left; padding-right 5px; padding-top 3px;
				.sep
					float left; width 3px; height 3px; margin 13px 0 0 10px; 
					background #b0b3ba; border-radius 3px; 
				.count
					float left; margin-left 10px; padding 5px 8px; color #fff; font-size 12px;
					background #b0b3ba; border-radius 3px;
	//=====
	.graph-content
		position relative; height 945px; background url("../img/main/graph-bg-rep.png") center 77px;
		.graph
			position absolute; width 600px; left 50%; top -40px; margin-left -300px; z-index 5;
			.remove
				position absolute; left 220px; top -5px;
				.icon
					display block; width 26px; height 17px;
					background url("../img/main/i-graph-remove.png") no-repeat; 
					.act
						width 26px; height 17px;
						background url("../img/main/i-graph-remove-a.png") no-repeat; 
			.add
				position absolute; right 220px; top -5px;
				.icon
					display block; width 26px; height 17px;
					background url("../img/main/i-graph-add.png") no-repeat; 
					.act
						width 26px; height 17px;
						background url("../img/main/i-graph-add-a.png") no-repeat;
			.graph-bg-full
				display none;
				z-index 12;
				position fixed; top 0%; left 0; width 100%; height 100%; full(); overflow hidden;
				background alpha(#f0f0f0, 60%); 
			.graph-inside
				position relative;
				.element
					width 20px; position absolute;
					.prev
						opacity .7;
						&:hover
							opacity 1; 
					.hidden-box
						z-index 22;
						display none;
						position absolute; left 0; margin-left -106px; bottom 100px; width 250px; full(); 
						padding 20px; 
						text-align center;
						background #fff;
						border: 1px solid #E2E2E2;
						border-radius 4px;
						box-shadow: 0px 0px 50px rgba(0, 0, 0, 0.1);
						&:after
							content " "; 
							position absolute; left 50%; bottom -50px; margin-left -14px; 
							width 28px; height 37px;
							background url("../img/main/i-map-cur.png") no-repeat; 
					h3
						color #7e8084; font-size 16px;
						margin-bottom 10px;
					p
						margin-bottom 10px;
						color #7e8084;
					p.blue
						margin-bottom 15px;
						color #3bb3c7; font-weight 400; font-size 14px;
				.element.active
					.prev
						position relative;
						z-index 12;
						opacity 1;		   
				.element-1
					left -1px; top 383px;
				.element-time
					z-index 10;
					display none;
					position absolute; 
					//left 0; 
					//bottom 100px; 
					width 90px;
					height 20px;
					//full(); 
					padding 20px; 
					text-align center;
					background #fff;
					border: 1px solid #E2E2E2;
					border-radius 4px;
					box-shadow: 0px 0px 50px rgba(0, 0, 0, 0.1);
					
		.ways
			position absolute; right 30px; top 400px; 
			ul
				display inline-block;
				lino();
				li
					display block; padding 0 0 10px 0;
					a
						display block; padding 5px 10px; color #fff; border-radius 50% 50% 50% 50%; anim-op();
						&:hover 
							opacity 0.8; tdn();
				li.pink a
					background #a574c3; 
				li.blue a
					background #3cb4c7; 
				li.blue-dark a
					background #378ed0; 
				li.green a
					background #46dfab;
				li.active
					a
						opacity 0.7;
				li.disable a 
					background #B0B3BA
		.recomends
			position absolute; left 30px; top 40px;
		.terms
			position absolute; right 30px; top 53px;
			ul.switch
				width 140px;
				lino();
				li
					float left;
					a
						color #a5a8ac; text-transform uppercase; semibold(); tdn(); anim();
				li.switch-con
					margin 1px 6px 0 6px; anim-op();
					.switch-line
						position relative; width 34px; height 18px;
						background #c8cbd0; border-radius 18px; cursor pointer;
						anim();
						.switch-dot
							position absolute; left 2px; top 2px; width 14px; height 14px; border-radius 14px; 
							background #ffffff; anim();
				li.switch-con.right
					.switch-dot
						left 18px; anim();
					.switch-line
						background #3cb4c7; 
				li.switch-con:hover
					opacity 0.8;
				li.active a
					color #3cb4c7; cursor pointer;


//	СЛАЙДЕР ОБРАЗОВАНИЯ / РАБОТЫ
// ===============================================
.works-time
	position relative; overflow hidden; background #fff; border-radius 3px;
	.hide-bg-r
		position absolute; right -20px; top 0; width 127px; height 98%; z-index 10;
		background url("../img/main/test-step-bg-hide-r.png") repeat-y; 
	.hide-bg-l
		position absolute; left -20px; top 0; width 127px; height 98%; z-index 10;
		background url("../img/main/test-step-bg-hide-l.png") repeat-y; 
	.slider-wrap
		.owl-stage-outer
			padding 0 20px 0 20px; full()
		.item
			float left; padding 0px 20px 30px 20px; full(); position relative;
			.ins
				padding-left 25px; margin-bottom 15px; border-left 2px solid #e1e1e1; 
			.icon
				position relative; display block; margin 0 0 15px 25px; width 35px; height 35px;
				&.educ
					background url("../img/main/i-circle-work.png") no-repeat;
				&.work
					background url("../img/main/i-circle-educ.png") no-repeat;
				.bg
					position absolute; left -25px; top 15px; width 17px; height 50px;
					background url("../img/main/work-time-bg-s.png") no-repeat; 
			.name
				padding-bottom 5px; font-size 16px; bold(); text-transform uppercase;
			.post
				padding-bottom 10px; color #3bb3c7; font 14px;
				&.cert
					padding-left 30px; background url("../img/main/i-cert.png") 0 0px no-repeat;
			.desc
				min-height 40px; padding-bottom 15px;
			.dates
				position relative; clearfix(); padding-top 20px;
				background url("../img/main/bg-blue.png") 0 top repeat-x; 
				.date
					position relative; padding 5px 10px; background #ededed; 
					&:after
						position absolute; width 0; height 0; top -7px; content " ";
						border-style solid; border-width 7px 0 0 7px;
						border-color transparent transparent transparent #d5d5d5;	
					&.right
						float right; border-radius 3px 0 3px 3px;
						.date-icon
							right -6px;	
					&.right:after
						right 0px;
						border-width 0 0 7px 7px;
						border-color transparent transparent #d5d5d5 transparent;	
					&.left
						float left; border-radius 0 3px 3px 3px;
						.date-icon
							left -6px;	
					&.left:after
						left 0px;	

				.date-icon
					position absolute; top -26px; width 14px; height 14px;
					background url("../img/main/circle-white-blue.png") no-repeat;
				.date-text
					padding-top 0px; font-size 12px; white-space nowrap;
					.year
						display inline-block; font-size 14px; semibold();
	.dots
		text-align center; padding-bottom 30px;
		ul
			display inline-block;
